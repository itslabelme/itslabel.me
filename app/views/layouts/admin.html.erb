<!doctype html>
<html lang="en">

  <!-- Head -->
  <%= render partial: 'layouts/common/head' %>

  <body class="font-roboto light_version">

    <!-- Page Loader -->
    <%= render partial: 'layouts/admin/loader' %>

    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>

    <div id="wrapper">
        <!-- Top Navigation -->
        <%= render partial: 'layouts/admin/navbar' %>

        <!-- Search Container -->
        <%#= render partial: 'layouts/admin/search' %>

        <!-- Left Sidebar -->
         <%= render partial: 'layouts/admin/left_sidebar' %>

        <!-- Main container -->
        <div id="main-content" class="mt-2">

          <!-- Show Success / Error / Info Messages If any set by the controller action -->
          <%#= flash_message(false) if defined?(flash) -%>
          <%= yield %>

        </div>
        <!-- End of Main container -->
        
    </div>


    <!-- Application Javascripts  -->
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- Vendor Javascripts  -->
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.js' %>
    <%#= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-tagsinput/0.8.0/bootstrap-tagsinput.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/Dropify/0.2.2/js/dropify.min.js' %>
    <%#= javascript_include_tag 'https://cdn.jsdelivr.net/npm/handsontable@latest/dist/handsontable.full.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/tinymce/5.1.6/tinymce.min.js' %>

    <!-- Lib for datatables -->
    <%= javascript_include_tag 'https://cdn.datatables.net/1.10.11/js/jquery.dataTables.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js' %>
    <%= javascript_include_tag 'https://cdn.datatables.net/fixedcolumns/3.2.4/js/dataTables.fixedColumns.min.js' %>
    <%= javascript_include_tag 'https://cdn.datatables.net/buttons/1.5.2/js/dataTables.buttons.min.js' %>
    <%= javascript_include_tag 'https://cdn.datatables.net/buttons/1.5.2/js/buttons.colVis.min.js' %>
    <%= javascript_include_tag 'https://cdn.datatables.net/buttons/1.5.2/js/buttons.flash.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/pdfmake.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/vfs_fonts.js' %>
    <%= javascript_include_tag 'https://cdn.datatables.net/buttons/1.5.2/js/buttons.html5.min.js' %>
    <%= javascript_include_tag 'https://cdn.datatables.net/buttons/1.5.2/js/buttons.print.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.1/moment.min.js' %>
     <!-- end of Lib for datatables -->
    
    <!-- Yielding Javascript Footer -->
    <script type="text/javascript">
      <%= yield :javascript_footer -%>
    </script>
    
  </body>
</html>
