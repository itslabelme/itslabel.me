
<% if @document.errors.blank? %>

  notifySuccess("<%= escape_javascript('Success!') %>", "<%= escape_javascript('Successfully Moved') %>");
  // window.location.reload();
  $("#doc_table").html("<%= escape_javascript(render(:partial=>'user/documents/index/table.html.erb')) %>");
<% else %>

  // Showing Growl Like Message
  notifyError("<%= escape_javascript(@notification[:title]) %>", "<%= escape_javascript(@notification[:message]) %>");


<% end %>



