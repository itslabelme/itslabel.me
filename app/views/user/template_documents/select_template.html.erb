<div class="container-fluid">
  <div class="block-header">
    <div class="row clearfix">
      <div class="col-md-6 col-sm-12">
        <h1>Select Template</h1>
        <p class="pt-2">Select Template to create Template Based Translation Document</p>
      </div>
      <div class="col-md-6 col-sm-12">
        <nav aria-label="breadcrumb" style="float:right">
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><%= link_to "Home", user_home_path %></li>
            <li class="breadcrumb-item"><%= link_to "Translations", user_documents_path(dt: "template") %></li>
            <li class="breadcrumb-item active">New Translation</li>
          </ol>
        </nav>
      </div>
    </div>
  </div>

  <div class="row clearfix">
    <div class="col-lg-12">
      <div class="card">
        <div class="body">
          <h5 class="mb-4">Choose a Template</h5>
          <%= render partial: 'user/template_documents/select_template/index' %>
        </div>
      </div>
    </div>
  </div>
</div>