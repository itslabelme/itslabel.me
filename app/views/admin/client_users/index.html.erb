<div class="container-fluid">

  <!-- Header -->
  <%= render partial: 'admin/client_users/index/header' %>
  
  <!-- Filters -->
  <%#= render partial: 'admin/client_users/index/filters' %>

  <!-- Controls -->
  <%= render partial: 'admin/client_users/index/controls' %>

  <div class="row clearfix">
    <div class="col-lg-12">
      <div class="card">
             <ul class="nav nav-tabs">
          <li class="nav-item"><a class="nav-link active show" data-toggle="tab" href="#CLIENT_LIST">Clients</a></li>
          
          
          <li class="nav-item" style='display: none;' id='LI_CLIENT_SHOW'><a class="nav-link" data-toggle="tab" href="#CLIENT_SHOW">View LabelTemplate</a></li>        
        </ul>
          <div class="tab-content mt-0">
          <!-- Table -->
          <div class="tab-pane active show" id="CLIENT_LIST">
              <%= render partial: 'admin/client_users/index/table' %>
                <!-- Pagination -->
          <% if @client_users .present? %>
            <%= paginate @client_users if @client_users.any? %>
          <% end  %>
          </div>
          
          
          <div class="tab-pane" id="CLIENT_SHOW">
             <%= render partial: 'admin/client_users/index/show' %>
          </div>
        </div>  
        <div class="table-responsive">
            
          <!-- Table -->
        

        
        </div>

      </div>
    </div>
  </div>
</div>