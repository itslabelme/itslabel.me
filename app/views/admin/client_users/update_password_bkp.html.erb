    <!-- Page Loader -->
<!doctype html>
<html lang="en">
<head>

  <title><%= page_title || "ITSLabel.me" -%></title>

  <%= meta_tags -%>
  <%= csrf_meta_tags -%>
  <%= csp_meta_tag -%>
  
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
  <meta name="description" content="">
  <meta name="keywords" content="">
  <meta name="author" content="">

  <!-- Favicons -->
  <link rel="shortcut icon" href="/favicon.ico", type ="image/x-icon">


  <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#f8ae4c">
  <meta name="msapplication-TileColor" content="#ffc40d">
  <meta name="theme-color" content="#0152a1">

  <meta name="msapplication-TileColor" content="#ffffff">
  <meta name="theme-color" content="#ffffff">

  <!-- Vendor Stylesheets -->
  <%= stylesheet_link_tag 'https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css' %>
  <%= stylesheet_link_tag 'https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css' %>
  <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/Dropify/0.2.2/css/dropify.min.css' %>
  <%#= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-tagsinput/0.8.0/bootstrap-tagsinput.css' %>
  <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/handsontable/7.3.0/handsontable.min.css' %>
  <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker3.min.css' %>
  <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.css' %>

  <!-- Application Stylesheets -->
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>

  <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/handsontable@latest/dist/handsontable.full.min.css">

  <!-- Hotjar Tracking Code for https://itslabel.me/ -->
  <% if Rails.env == "production" %>
    <script>
        (function(h,o,t,j,a,r){
          h.hj=h.hj||function(){(h.hj.q=h.hj.q||[]).push(arguments)};
          h._hjSettings={hjid:1902324,hjsv:6};
          a=o.getElementsByTagName('head')[0];
          r=o.createElement('script');r.async=1;
          r.src=t+h._hjSettings.hjid+j+h._hjSettings.hjsv;
          a.appendChild(r);
        })(window,document,'https://static.hotjar.com/c/hotjar-','.js?sv=');
    </script>
  <% end %>
  
</head>



  <body class="font-roboto light_version">

<div class="page-loader-wrapper">
    <div class="loader">
        <div class="bar1"></div>
        <div class="bar2"></div>
        <div class="bar3"></div>
        <div class="bar4"></div>
        <div class="bar5"></div>
    </div>
</div>


<div class="auth-main2">
  <div class="vivify its-login-container">
    <div class="container">
      <div class="row">
        <div class="col-md-12" style="display: none;">
          <div class="auth_brand m-t-60" style="text-align: center;">
            <a class="navbar-brand" href="javascript:void(0);">
              <!-- <img src="../assets/images/itsme-logo-1.png" width="142" height="60" class="d-inline-block" alt=""> -->
              <%= image_tag "itsme-logo-1.png", alt: "Login Image", class: "d-inline-block", width: "142", height: "60" %>
            </a>
          </div>
          
        </div>
        <div class="col-md-12">
          <div class="row">
            <div class="col-md-10" style="margin: 0 auto">
              <div class="card m-t-20">
                <div class="body its-login-body">
                  <div class="row">
                    <div class="col-md-6 its-login-bg">
                        <div style="width: 100%;height: 100%;display: table;">
                            <div style="width: 100%;height: 100%;display: table-cell;vertical-align: middle;">

                              <%= image_tag "keyart-2.png", alt: "Login Image", style: "max-width: 80%;" %>

                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 its-login-form">
                      <div class="row its-login-header">
                        <!-- Mobile Logo -->
                        <div class="col-md-4 hide-desk show-tab">
                          <div class="auth_brand"> <a class="navbar-brand" href="javascript:void(0);">
                            <%= image_tag "itsme-logo-1.png", alt: "Login Image", class: "d-inline-block" %>
                          </a> </div>
                        </div>

                        <div class="col-md-8">
                          <div class="mb-3">
                            <p class="lead">Reset Password</p>
                          </div>
                        </div>
                        <div class="col-md-4 hide-mob hide-tab">
                          <div class="auth_brand">
                            <a class="navbar-brand" href="javascript:void(0);">
<!--                               <img src="../assets/images/itsme-logo-1.png" class="d-inline-block" alt="">
 -->                              <%= image_tag "itsme-logo-1.png", alt: "Login Image",  									 class: "d-inline-block" %>
                            </a>
                          </div>
                        </div>


                      </div>



                    <%= form_for(resource, as: resource_name, url: admin_forgot_password_path, html: { method: :post, class: "form", id: "register-form" }) do |f| %> 
                          
                          <div class="mb-3">
                              <p>Please enter your  password</p>
                          </div>
                          <div class="form-group">
                            <div class="input-group">
                             <span class="input-group-addon"><i class="glyphicon glyphicon-envelope color-blue"></i></span>
                               <%= f.label :password, "New password" %><br />
                              <% if @minimum_password_length %>
                                <em>(<%= @minimum_password_length %> characters minimum)</em><br />
                              <% end %>
                              <%= f.password_field :password, autofocus: true, autocomplete: "new-password" %>
                            </div>
                          </div>
                            <div class="form-group">
                            <div class="input-group">
                             <span class="input-group-addon"><i class="glyphicon glyphicon-envelope color-blue"></i></span>                          
                              <%= f.label :password_confirmation, "Confirm new password" %><br />
                              <%= f.password_field :password_confirmation, autocomplete: "new-password" %>

                          <div class="form-group">
                            <input name="recover-submit" class="btn btn-lg btn-primary btn-block" value="Reset Password" type="submit">
                          </div>
                            <% end %>
        
                    </div>
                  </div>
                </div>
              </div>
              
            </div>
            
          </div>
          
        </div>
        
      </div>
      
    </div>
  </div>
</div>
  <%= yield %>

    <!-- Application Javascripts  -->
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- Vendor Javascripts  -->
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.js' %>
    <%#= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-tagsinput/0.8.0/bootstrap-tagsinput.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/Dropify/0.2.2/js/dropify.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/tinymce/5.1.6/tinymce.min.js' %>
    
  </body>
</html>


