<div class="container-fluid">

  <!-- Header -->
  <%= render partial: 'admin/label_templates/index/header' %>

  <!-- Controls -->
  <%= render partial: 'admin/label_templates/index/controls' %>

  <div class="row clearfix">
    <div class="col-lg-12">
      
      <div class="card">
        <ul class="nav nav-tabs">
          <li class="nav-item"><a class="nav-link active show" data-toggle="tab" href="#TEMPLATES_LIST">LabelTemplates</a></li>
          
          <li class="nav-item"  style='display: none;' id='LI_TEMPLATES_NEW_FORM'><a class="nav-link" data-toggle="tab" href="#TEMPLATES_NEW_FORM">Add  a LabelTemplate</a></li>        
          <li class="nav-item" style='display: none;' id='LI_TEMPLATES_EDIT_FORM'><a class="nav-link" data-toggle="tab" href="#TEMPLATES_EDIT_FORM">Edit LabelTemplate</a></li>        
        </ul>

        <div class="tab-content mt-0">
          <!-- Table -->
          <div class="tab-pane active show" id="TEMPLATES_LIST">
            <%= render partial: 'admin/label_templates/index/table' %>
          </div>
          
          <!-- New Form -->
          <div class="tab-pane" id="TEMPLATES_NEW_FORM">
            <%#= render partial: 'admin/label_templates/form/form' %>
          </div>

          <!-- Edit Form -->
          <div class="tab-pane" id="TEMPLATES_EDIT_FORM">
          </div>
        </div>            
      </div>
    </div>
  </div>
</div>

<%= content_for :javascript_footer do %>
  <%#= render :partial=>"admin/label_templates/index/table.js.erb"  %>
<% end %>